{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "es6": true,
    "jest/globals": true
  },
  "parserOptions": {
    "ecmaVersion": 8,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "no-console": "warn"
  },
  "extends": [
    "eslint:recommended",
    "plugin:jest/recommended",
  ],
  "plugins": [
    "import",
    "jest",
  ],
}
